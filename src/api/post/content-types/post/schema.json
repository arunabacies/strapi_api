{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "site": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::site.site",
      "inversedBy": "posts"
    },
    "title": {
      "type": "string"
    },
    "content": {
      "type": "text"
    },
    "url": {
      "type": "string"
    },
    "parent_id": {
      "type": "integer"
    },
    "is_active": {
      "type": "boolean"
    },
    "post_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-category.post-category",
      "mappedBy": "post"
    },
    "feature_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "post_detail": {
      "type": "richtext"
    }
  }
}
